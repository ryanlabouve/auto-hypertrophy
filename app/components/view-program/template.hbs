<div>
  <div class="invisible bg-grey p-1 m-y-4" data-test-program-name>{{macrocycle.name}}</div>
  {{#each macrocycle.mesocycles as |mesocycle|}}
    {{#each mesocycle.microcycles as |microcycle|}}
      {{#each (range 1 13) as |n|}}
        <div class="text-2xl">Week {{n}}</div>
        {{#each microcycle.sessions as |session|}}
          {{view-workout
            session=session
            loggedSession=(find-by 'week' n session.loggedSessions)
            week=n
          }}
        {{/each}}
      {{/each}}
    {{/each}}
  {{/each}}
</div>