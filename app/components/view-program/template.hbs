<div>
  <div class="bg-grey p-1 m-y-4" data-test-program-name>{{macrocycle.name}}</div>
  {{#each macrocycle.mesocycles as |mesocycle|}}
    {{#each mesocycle.microcycles as |microcycle|}}
      {{#each (range 1 12) as |n|}}
        <div class="text-2xl">Week {{n}}</div>
        {{#each microcycle.sessions as |session|}}
          {{view-workout session=session}}
        {{/each}}
      {{/each}}
    {{/each}}
  {{/each}}
</div>