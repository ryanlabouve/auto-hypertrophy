<table class="w-full text-center">
  <tr>
    <td>Set No.</td>
    <td>Weight</td>
    <td>Reps</td>
    <td>RPE</td>
    <td></td>
  </tr>
  {{#each this.loggedSets as |loggedSet|}}
    <ViewMove::SingleSet
      @addPound={{action (perform this.addPound loggedSet)}}
      @subtractPound={{action (perform this.subtractPound loggedSet)}}
      @addRep={{action (perform this.addRep loggedSet)}}
      @subtractRep={{action (perform this.subtractRep loggedSet)}}
      @loggedSet={{loggedSet}} />
  {{else}}
    <tr>
      <td colspan=100>
        No sets loaded
      </td>
    </tr>
  {{/each}}
</table>