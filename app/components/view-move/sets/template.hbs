<table class="w-full text-center">
  <tr>
    <td>
      {{!col completed|skiped|failed}}
    </td>
    <td>Set No.</td>
    <td>Weight</td>
    <td>Reps</td>
    <td>RPE</td>
    <td>
      {{! kebab }}
    </td>
  </tr>
  {{#each this.loggedSets as |loggedSet|}}
    <ViewMove::SingleSet
      @addPound={{action (perform this.addPound loggedSet 1)}}
      @addFivePound={{action (perform this.addPound loggedSet 5)}}
      @addTenPound={{action (perform this.addPound loggedSet 10)}}
      @subtractPound={{action (perform this.subtractPound loggedSet 1)}}
      @subtractFivePound={{action (perform this.subtractPound loggedSet 5)}}
      @subtractTenPound={{action (perform this.subtractPound loggedSet 10)}}
      @addRep={{action (perform this.addRep loggedSet)}}
      @subtractRep={{action (perform this.subtractRep loggedSet)}}
      @completeSet={{action (perform this.completeSet loggedSet)}}
      @failSet={{action (perform this.failSet loggedSet)}}
      @skipSet={{action (perform this.skipSet loggedSet)}}
      @loggedSet={{loggedSet}} />
  {{else}}
    <tr>
      <td colspan=100>
        No sets loaded
      </td>
    </tr>
  {{/each}}
</table>